<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.controllers.SaveController" spacing="10" style="-fx-padding: 20">
    <Label text=" Do you want to save this reservation ?" style="-fx-font-size: 14px;"></Label>
    <HBox spacing="10" alignment="center">
        <JFXButton style="-fx-text-fill:WHITE;-fx-background-color:#5264AE;-fx-font-size:14px;"
                   prefWidth="100" prefHeight="40" buttonType="RAISED" text="Save" onAction="#save"></JFXButton>
        <JFXButton style="-fx-background-color:WHITE;-fx-font-size:14px;"
                   prefWidth="100" prefHeight="40" buttonType="RAISED" fx:id="cancelButton" text="Cancel"
                   onAction="#cancel"></JFXButton>

    </HBox>
</VBox>
