<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns:fx="http://javafx.com/fxml" xmlns="http://javafx.com/javafx" fx:controller="gui.controllers.OrderController"
      style="-fx-padding: 10">
    <ScrollPane style="-fx-padding: 10" fx:id="scrollPane" maxHeight="500"/>
    <HBox alignment="CENTER" spacing="10" style="-fx-padding: 10">
        <JFXButton style="-fx-text-fill:WHITE;-fx-background-color:#5264AE;-fx-font-size:14px;"
                   prefWidth="100" prefHeight="40" buttonType="RAISED" fx:id="buttonOk" text="OK"
                   onMouseClicked="#buttonOkClicked"/>
        <JFXButton style="-fx-background-color:WHITE;-fx-font-size:14px;"
                   prefWidth="100" prefHeight="40" buttonType="RAISED" fx:id="buttonCancel" text="Cancel"
                   onMouseClicked="#buttonCancelClicked"/>
    </HBox>
</VBox>
